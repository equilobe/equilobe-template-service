{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "Path": "logs/log-.jsonl",
                    "RollingInterval": "Day",
                    "RollOnFileSizeLimit": true,
                    "Formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
                }
            }
        ]
    },
    "CorsAllow": [
        "http://localhost:4200"
    ],
    "AllowedHosts": "*",
    "Auth0": {
        "Authority": "",
        "Audience": ""
    },
    "ConnectionStrings": {
        "DefaultConnection": "Host=127.0.0.1;Database=local-equilobe-template-service-db;User Id=postgres;Password=postgres;"
    }
}